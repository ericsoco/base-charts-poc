[ignore]
.*/node_modules/.*[^(package)]\.json$
.*/node_modules/.*/test/.*

[include]
./src/

[libs]

[lints]
unclear-type=error
untyped-import=error
untyped-type-import=error
sketchy-null=off

[options]
include_warnings=true
suppress_comment=\\(.\\|\n\\)*\\$AssertError
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
esproposal.optional_chaining=enable

[strict]
